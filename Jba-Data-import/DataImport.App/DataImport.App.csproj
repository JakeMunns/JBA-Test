        <ImplicitUsings>enable</ImplicitUsings>
        <UseWPF>true</UseWPF>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\DataImport.Conversion\DataImport.Conversion.csproj" />
      <ProjectReference Include="..\DataImport.Dal\DataImport.Dal.csproj" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="favicon(1).ico" />
    </ItemGroup>

    <ItemGroup>
      <Resource Include="Resources\favicon.ico">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
    </ItemGroup>

</Project>
